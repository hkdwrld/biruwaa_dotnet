@model IEnumerable<Product>
<h1>  Products  </h1>
<a href="/Product/Create" class="btn btn-primary">Create</a>
<table class="table table-borderd table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id </td>
                <td>@item.Name</td>
            </tr>
        }
    </tbody>
</table>